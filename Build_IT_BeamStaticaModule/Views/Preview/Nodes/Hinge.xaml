<UserControl x:Class="Build_IT_BeamStaticaModule.Views.Preview.Nodes.Hinge"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Build_IT_BeamStaticaModule.Views.Preview.Nodes"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Canvas>
        <Ellipse Width="20"
                 Height="20"
                 Canvas.Left="-10"
                 Canvas.Top="-10"
                  Fill="{StaticResource GrayBrush}"
                  Stroke="{StaticResource WhiteBrush}"
                  StrokeThickness="1"/>
    </Canvas>
</UserControl>
