<UserControl x:Class="Build_IT_BeamStaticaModule.Views.Preview.PointLoads.NormalLoad"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Build_IT_BeamStaticaModule.Views.Preview.PointLoads"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Canvas>
        <Canvas Canvas.Top="-20">
        <Line X1="-25" X2="0"
          Y1="0" Y2="0"
          StrokeThickness="2"
          Stroke="{StaticResource OrangeBrush}"/>

        <Line X1="-10" X2="0"
          Y1="5" Y2="0"
          StrokeThickness="3"
          Stroke="{StaticResource OrangeBrush}"/>
        <Line X1="-10" X2="0"
          Y1="-5" Y2="0"
          StrokeThickness="3"
          Stroke="{StaticResource OrangeBrush}"/>
        </Canvas>
    </Canvas>
</UserControl>
